{
  "name": "Webpack_Demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "PRODUCTION=true webpack --config webpack.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "build:profile": "PROFILER=true PRODUCTION=true webpack",
    "start": "SERVER_RENDER=true node dist/server/server.js",
    "start:client": "webpack serve --open",
    "start:server": "SERVER_RENDER=true nodemon --config nodemon.json dist/server/server.js",
    "dev:iso": "yarn clean && SERVER_RENDER=true npm-run-all --parallel dev:iso:client dev:iso:server",
    "dev:iso:client": "yarn run build:client --watch",
    "dev:iso:server": "yarn run build:server --watch",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "serialize-javascript": "^5.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
